{
  "consul": "{{ .CONSUL_URL }}:8500",
  "preStart": [
    "sed", "-i",
    "-e", "s/CONTAINERPILOT_CONSUL_IP/{{ .CONTAINERPILOT_CONSUL_IP }}/",
    "-e", "s/CONSUL_URL/{{ .CONSUL_URL }}/",
    "/etc/consul.json"
  ],
  "services": [
    {
      "name": "consul",
      "port": 8500,
      "poll": 10,
      "ttl": 25,
      "tags": ["http"]
    }
  ]
}
